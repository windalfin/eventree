[run]
source = eventree
omit =
    */migrations/*
    */tests/*
    */admin.py
    */apps.py
    */urls.py
    manage.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    raise NotImplementedError
    if settings.DEBUG
    if __name__ == .__main__.:

[html]
directory = htmlcov
